{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": ["default"],
    "sharedGlobals": []
  },
  "nxCloudAccessToken": "YTJlZDY4NmItOGU4My00YTcyLWI0MmUtYTY2OTFjOTg2M2NhfHJlYWQtd3JpdGU=",
  "plugins": ["@nxtensions/astro"],
  "targetDefaults": {
    "@nxtensions/astro:build": {
      "inputs": ["production", "^production"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "cache": true
    },
    "@nxtensions/astro:check": {
      "inputs": ["production", "^production"],
      "cache": true
    },
    "@nxtensions/astro:preview": {
      "dependsOn": ["build"]
    }
  },
  "defaultProject": "nx-project-example"
}
